{
  "name": "nx-workspace",
  "version": "independent",
  "license": "MIT",
  "scripts": {
    "clean-modules": "lerna clean",
    "nx-reset": "nx reset",
    "build": "nx run-many -t build",
    "build:since": "nx affected -t build",
    "lint": "nx run-many -t lint",
    "lint-fix": "nx run-many -t lint-fix",
    "test": "nx run-many -t test",
    "test:since": "nx affected -t test",
    "serve": "nx run-many -t serve",
    "release": "cross-env HUSKY_BYPASS=true lerna publish"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^17.8.0",
    "@commitlint/config-conventional": "^17.8.0",
    "@nx/devkit": "^17.0.2",
    "@nx/eslint": "^17.0.2",
    "@nx/jest": "^17.0.2",
    "@nx/js": "^17.0.2",
    "@nx/web": "^17.0.2",
    "@nx/webpack": "^17.0.2",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@types/jest": "^29.4.0",
    "@types/node": "18.7.1",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "lerna": "^7.4.1",
    "nx": "^17.0.2",
    "nx-cloud": "latest",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3",
    "verdaccio": "^5.0.4"
  },
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "volta": {
    "node": "18.17.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "[[ -n $HUSKY_BYPASS ]] || commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "nx": {
    "includedScripts": []
  }
}
